parameters:
    level: 6
    paths:
        - ../../src/
        - ../../examples/
    treatPhpDocTypesAsCertain: false
    ignoreErrors:
        -   # @see https://github.com/phpstan/phpstan/issues/5952
            message: "#^Dead catch \\- Exception is never thrown in the try block\\.$#"
            paths:
                - ../../examples/
